#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ryu/app/miniature-garbanzo/node_modules/JSONStream/node_modules:/home/ryu/app/miniature-garbanzo/node_modules:/home/ryu/app/node_modules:/home/ryu/node_modules:/home/node_modules:/node_modules:/home/ryu/app/miniature-garbanzo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ryu/app/miniature-garbanzo/node_modules/JSONStream/node_modules:/home/ryu/app/miniature-garbanzo/node_modules:/home/ryu/app/node_modules:/home/ryu/node_modules:/home/node_modules:/node_modules:/home/ryu/app/miniature-garbanzo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../JSONStream/bin.js" "$@"
else
  exec node  "$basedir/../JSONStream/bin.js" "$@"
fi
